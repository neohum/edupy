{
  "name": "edupy",
  "version": "1.0.0",
  "description": "Educational Python Learning Platform",
  "scripts": {
    "dev": "concurrently -n \"FRONTEND,BACKEND\" -c \"cyan,magenta\" \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && python main.py",
    "frontend": "cd frontend && npm run dev",
    "backend": "cd backend && python main.py",
    "install:all": "npm install && cd frontend && npm install",
    "install:backend": "cd backend && pip install -r requirements.txt",
    "build": "cd frontend && npm run build",
    "setup": "npm run install:all && npm run install:backend"
  },
  "keywords": [
    "python",
    "education",
    "typing",
    "learning"
  ],
  "author": "neohum",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}

